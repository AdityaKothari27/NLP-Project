      "source": [
        "from transformers import BertTokenizer, BertForMaskedLM\n",
        "from transformers import Trainer, TrainingArguments\n",
        "import torch\n",
        "from torch.utils.data import Dataset"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Custom Dataset class for training\n",
        "class CustomDataset(Dataset):\n",
        "    def __init__(self, tokenizer, data, max_length):\n",
        "        self.tokenizer = tokenizer\n",
        "        self.data = data\n",
        "        self.max_length = max_length\n",
        "\n",
        "    def __len__(self):\n",
        "        return len(self.data)\n",
        "\n",
        "    def __getitem__(self, idx):\n",
        "        item = self.data[idx]\n",
        "        encoding = self.tokenizer(item, padding='max_length', truncation=True, max_length=self.max_length, return_tensors='pt')\n",
        "        input_ids = encoding['input_ids'].squeeze(0)\n",
        "        labels = input_ids.clone()\n",
        "        labels[labels == self.tokenizer.mask_token_id] = -100\n",
        "        return {'input_ids': input_ids, 'labels': labels}\n",
        "\n",
        "# Loading the pre-trained BERT model and tokenizer\n",
        "model_name = 'bert-base-uncased'\n",
        "tokenizer = BertTokenizer.from_pretrained(model_name)\n",
        "model = BertForMaskedLM.from_pretrained(model_name)\n",
        "\n",
        "# Custom training data\n",
        "custom_dataset = [\n",
        "    \"My car is really fast.\",\n",
        "    \"My car is extremely fast.\",\n",
        "    \"My car is incredibly fast.\",\n",
        "]\n",
        "\n",
        "# Defining the maximum sequence length\n",
        "max_length = 20\n",
        "\n",
        "# Creating an instance of the CustomDataset\n",
        "train_dataset = CustomDataset(tokenizer, custom_dataset, max_length)\n",
        "\n",
        "# Defining the training arguments\n",
        "output_dir = \"./custom_model_save\"\n",
        "training_args = TrainingArguments(\n",
        "    output_dir=output_dir,\n",
        "    num_train_epochs=3,\n",
        "    per_device_train_batch_size=16,\n",
        "    save_total_limit=2,\n",
        ")\n",
        "\n",
        "# Fine-tuning the model on the custom dataset\n",
        "trainer = Trainer(\n",
        "    model=model,\n",
        "    args=training_args,\n",
        "    train_dataset=train_dataset,\n",
        ")\n",
        "\n",
        "trainer.train()\n",
        "trainer.model.save_pretrained(output_dir)\n",
        "\n",
        "# Loading the fine-tuned model for prediction\n",
        "from transformers import BertForMaskedLM, BertTokenizer, pipeline\n",
        "\n",
        "custom_model_path = output_dir\n",
        "custom_model = BertForMaskedLM.from_pretrained(custom_model_path)\n",
        "tokenizer = BertTokenizer.from_pretrained(model_name)\n",
        "unmasker = pipeline('fill-mask', model=custom_model, tokenizer=tokenizer)"
      ],
